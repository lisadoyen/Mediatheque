<!DOCTYPE html>

<title>Entite index</title>

{% block body %}
    <h1>Entite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entite in entites %}
            <tr>
                <td>{{ entite.id }}</td>
                <td>{{ entite.nom }}</td>
                <td>{{ entite.prenom }}</td>
                <td>
                    <a href="{{ path('entite_show', {'id': entite.id}) }}">show</a>
                    <a href="{{ path('entite_edit', {'id': entite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('entite_new') }}">Create new</a>
{% endblock %}
