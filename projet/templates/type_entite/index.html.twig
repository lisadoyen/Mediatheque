<!DOCTYPE html>

<title>TypeEntite index</title>

{% block body %}
    <h1>TypeEntite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_entite in type_entites %}
            <tr>
                <td>{{ type_entite.id }}</td>
                <td>{{ type_entite.libelle }}</td>
                <td>
                    <a href="{{ path('type_entite_show', {'id': type_entite.id}) }}">show</a>
                    <a href="{{ path('type_entite_edit', {'id': type_entite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_entite_new') }}">Create new</a>
{% endblock %}
